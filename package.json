{
    "name": "mkdocs-material-extras",
    "version": "0.0.1",
    "description": "mkdocs material extras",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "build": "bash build.sh",
        "serve": "mkdocs serve",
        "lint": "remark ./docs/**/*.md --quiet --frail",
        "format": "remark . --quiet --frail --output",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dup4/mkdocs-material-extras.git"
    },
    "keywords": [
        "mkdocs",
        "material",
        "theme"
    ],
    "author": {
        "name": "Dup4",
        "email": "lyuzhi.pan@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Dup4/mkdocs-material-extras/issues"
    },
    "homepage": "https://github.com/Dup4/mkdocs-material-extras#readme",
    "dependencies": {
        "mkdocs-render-math-ssr": "^1.0.5",
        "remark-clang-format": "^1.9.3",
        "remark-cli": "^10.0.1",
        "remark-copywriting-correct": "^0.5.0",
        "remark-details": "^4.0.4",
        "remark-footnotes": "^4.0.1",
        "remark-lint-final-newline": "^2.1.1",
        "remark-lint-no-tabs": "^3.1.1",
        "remark-math": "^5.1.1",
        "remark-math-space": "^3.0.0",
        "remark-preset-lint-markdown-style-guide": "^5.1.2"
    }
}
